{
  "family": "paneltask",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::442042528233:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::442042528233:role/ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "containerDefinitions": [
    {
      "name": "panelcontainer",
      "image": "442042528233.dkr.ecr.us-east-1.amazonaws.com/panelapprepo:1737826751",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5006,
          "hostPort": 5006,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/paneltask",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
